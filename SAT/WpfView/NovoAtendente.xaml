<Window x:Class="WpfView.NovoAtendente"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfView"
        mc:Ignorable="d"
        Title="NovoAtendente" Height="290" Width="346.834">
    <Grid Margin="0,0,2,0" Background="#FFABC1C9">
        <Button x:Name="btnCriar" Content="Criar" HorizontalAlignment="Left" Margin="120,200,0,0" VerticalAlignment="Top" Width="75" Click="btnCriar_Click"/>
        <Label Content="Usuario" HorizontalAlignment="Left" Margin="38,50,0,0" VerticalAlignment="Top" Width="60"/>
        <Label Content="Senha" HorizontalAlignment="Left" Margin="38,100,0,0" VerticalAlignment="Top" Width="60"/>
        <TextBox x:Name="txtUsuario" HorizontalAlignment="Left" Height="23" Margin="120,50,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="178"/>
        <PasswordBox x:Name="pswSenha" HorizontalAlignment="Left" Margin="120,100,0,0" VerticalAlignment="Top" Width="178"/>
        <PasswordBox x:Name="pswConfirmarSenha" HorizontalAlignment="Left" Margin="120,150,0,0" VerticalAlignment="Top" Width="178"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="38,150,0,0" VerticalAlignment="Top" Width="60"/>

    </Grid>
</Window>
